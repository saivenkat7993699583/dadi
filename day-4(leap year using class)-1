#write a program to print given year is leap year or not using class

class Leap:
   def year(self,n):
      if n%400==0:
         print("Given year is Leap year")
      elif n%4==0 and n%100!=0:
         print("Given year is not a Leap year")
      else:
         print("NOT")
y=int(input())
l=Leap()
print(l.year(y))
